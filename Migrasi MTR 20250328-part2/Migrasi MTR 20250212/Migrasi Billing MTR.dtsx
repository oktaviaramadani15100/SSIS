              refId="Package\Data Flow Task\OLE DB Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              localeId="1033"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select NEWID() billingno, 
paymentdate as Billingdate,
ISNULL(DATEADD(MONTH, 1, duedate), GETDATE()) AS paidtodate,
 * from
(
SELECT DISTINCT PLMNTH, b.Policyno, PISACT, c.PaymentId
    , PISCUR, PISAMX, PISAFX    , a.paymentdate, newduedate 'duedate'
    , 'M' ModeId, 'DBB' MethodId, 'PAI' StatusId, 'PRM' TypeId, 1 'MonthsPaid'
    , 1 'bankid', 'U' Source, CONCAT(CONVERT(VARCHAR, a.newduedate, 3),'-',b.POLICYNO) Reff
    , getdate() 'createdate', 'migrationma' 'createby', PISAMX 'basicpremium'
    from DMTM_MTR.dbo.TFPMMLD a
    inner join (select policyno, refferenceno from DMTM_New..policy where PRODUCTID in ('TRMIDR', 'TABINS_USD')
        and createby in ('migration','migrationma')
        and (year(effectivedate) = ? and month(EFFECTIVEDATE)= ?) ) b on a.PISACT COLLATE SQL_Latin1_General_CP1_CI_AS = b.REFFERENCENO COLLATE SQL_Latin1_General_CP1_CI_AS
    left join (select paymentid, policyno, DUEDATE from DMTM..PAYMENT) c on c.POLICYNO = b.POLICYNO and c.DUEDATE = a.newduedate
    left join (select BILLINGNO, policyno, DUEDATE, PAYMENTID from DMTM..BILLING) d on d.POLICYNO = b.POLICYNO and c.PAYMENTID = d.PAYMENTID and d.DUEDATE = a.newduedate
    WHERE LEFT(POPISO,4) = ?
        and (pismtd &gt;= 1900001
        and pismtd &lt; 2023091)
        and pisdob &gt;= 1900001
        and pisopd &gt;= 1900001
        and month(a.effectivedate) = ?
        and d.BILLINGNO is null
) x order by x.paymentdate desc</property>
